            <embedded>true</embedded>
            <script-type>
                <id>0</id>
                <inactive>false</inactive>
                <lock-version>0</lock-version>
                <last-updated>2013-11-07T19:43:23+00:00</last-updated>
                <last-update-user>SYSTEM</last-update-user>
                <created>2013-11-07T19:43:23+00:00</created>
                <create-user>SYSTEM</create-user>
                <name>Bash Script</name>
                <description>Bash shell script.</description>
                <name-message-key>type.script.bash.name</name-message-key>
                <description-message-key>type.script.bash.description</description-message-key>
                <supported-os-details>
                    <id>0</id>
                    <inactive>false</inactive>
                    <lock-version>0</lock-version>
                    <last-updated>2013-11-07T19:43:24+00:00</last-updated>
                    <last-update-user>SYSTEM</last-update-user>
                    <created>2013-11-07T19:43:24+00:00</created>
                    <create-user>SYSTEM</create-user>
                    <os-family>
                        <id>0</id>
                        <inactive>false</inactive>
                        <lock-version>0</lock-version>
                        <last-updated>2013-11-07T19:43:23+00:00</last-updated>
                        <last-update-user>SYSTEM</last-update-user>
                        <created>2013-11-07T19:43:23+00:00</created>
                        <create-user>SYSTEM</create-user>
                        <name>Linux</name>
                        <description>Linux based operating systems.</description>
                        <meta-tag-type>
                            <id>0</id>
                            <inactive>false</inactive>
                            <lock-version>0</lock-version>
                            <last-updated>2013-11-07T19:43:23+00:00</last-updated>
                            <last-update-user>SYSTEM</last-update-user>
                            <created>2013-11-07T19:43:23+00:00</created>
                            <create-user>SYSTEM</create-user>
                            <name>OS Family</name>
                            <description>Operating System (OS) Family.</description>
                            <meta-tag-enum>OS_FAMILY</meta-tag-enum>
                            <user-managed-type>false</user-managed-type>
                        </meta-tag-type>
                        <os-family>LINUX</os-family>
                    </os-family>
                    <supports-array-props>true</supports-array-props>
                    <supports-output-props>true</supports-output-props>
                    <exec-command>source &lt;exec-script&gt; &lt;script-file&gt; &lt;output-properties-file&gt;</exec-command>
                    <exec-script>trap "APPDEXITSTATUS=\$?;printenv &gt; $2;exit $APPDEXITSTATUS" EXIT
#Start of Json Parser

#result of getValueFromJson is saved in global variable $JSONVALUE
JSONVALUE=""

#use 2 arrays to simulate associate array in Bash 3.x
declare -a pathArray
declare -a valueArray

function _appd_pathMap_add () {
    local path
    local value
    path=$1
    value=$2
    pathArray=("${pathArray[@]}" "$path")
    valueArray=("${valueArray[@]}" "$value")
}

function _appd_pathMap_init () {
    unset pathArray
    unset valueArray
    declare -a pathArray
    declare -a valueArray
}

function _appd_pathMap_get () {
    local path=$1
    local length=$((${#pathArray[@]}-1))
    for i in `eval echo "{0..$length}"`;
    do
        if [ "${pathArray[$i]}" == "$path" ];
            then echo "${valueArray[$i]}"; return;
        fi
    done;
    _appd_throw_error "Key path $path not found"
}

function _appd_pathMap_output () {
    local length=$((${#pathArray[@]}-1))
    for i in `eval echo "{0..$length}"`;
    do
        echo "${pathArray[$i]} values ${valueArray[$i]}"
    done;
}

function _appd_throw_error () {
    echo "$*" &gt;&amp;2
    exit 1
}

function _appd_awk_egrep () {
    local pattern_string=$1

    gawk '{
        while ($0) {
            start=match($0, pattern);
            token=substr($0, start, RLENGTH);
            print token;
            $0=substr($0, start+RLENGTH);
        }
    }' pattern=$pattern_string
}

function _appd_tokenize () {
    local GREP
    local ESCAPE
    local CHAR

    if echo "test string" | egrep -ao --color=never "test" &amp;&gt;/dev/null
    then
        GREP='egrep -ao --color=never'
    else
        GREP='egrep -ao'
    fi

    if echo "test string" | egrep -o "test" &amp;&gt;/dev/null
    then
        ESCAPE='(\\[^u[:cntrl:]]|\\u[0-9a-fA-F]{4})'
        CHAR='[^[:cntrl:]"\\]'
    else
        GREP=_appd_awk_egrep
        ESCAPE='(\\\\[^u[:cntrl:]]|\\u[0-9a-fA-F]{4})'
        CHAR='[^[:cntrl:]"\\\\]'
    fi


    local STRING="\"$CHAR*($ESCAPE$CHAR*)*\""
    local NUMBER='-?(0|[1-9][0-9]*)([.][0-9]*)?([eE][+-]?[0-9]*)?'
    local KEYWORD='null|false|true'
    local SPACE='[[:space:]]+'
    $GREP "$STRING|$NUMBER|$KEYWORD|$SPACE|."  | egrep -v "^$SPACE$"
}

function _appd_parse_array () {
    local index=0
    local ary=''
    read -r token
    case "$token" in
        ']');;
        *)
            while :
            do
                _appd_parse_value "$1" "[$index]"
                index=$((index+1))
                ary="$ary""$value"
                read -r token
                case "$token" in
                    ']') break ;;
                    ',') ary="$ary," ;;
                    *) _appd_throw_error "EXPECTED , or ] GOT ${token:-EOF}" ;;
                esac
                read -r token
            done
    esac
    value=`printf '[%s]' "$ary"`
    :
}

function _appd_parse_object () {
    local key
    local obj=''
    read -r token
    case "$token" in
        '}') ;;
        *)
            while :
            do
                case "$token" in
                    '"'*'"') key=$token ;;
                    *) _appd_throw_error "EXPECTED string GOT ${token:-EOF}" ;;
                esac
                read -r token
                case "$token" in
                    ':') ;;
                    *) _appd_throw_error "EXPECTED : GOT ${token:-EOF}" ;;
                esac
                read -r token
                _appd_parse_value "$1" "${key:1:$((${#key}-2))}"
                obj="$obj$key:$value"
                read -r token
                case "$token" in
                    '}') break ;;
                    ',') obj="$obj," ;;
                    *) _appd_throw_error "EXPECTED , or } GOT ${token:-EOF}" ;;
                esac
                read -r token
            done
    esac
    value=`printf '{%s}' "$obj"`
    :
}

function _appd_parse_value () {
    local jpath
    [[ "$2" == \[* ]] &amp;&amp; jpath="${1:+$1}$2" || jpath="${1:+$1.}$2"
    case "$token" in
        '{') _appd_parse_object "$jpath" ;;
        '[') _appd_parse_array  "$jpath" ;;
        # At this point, the only valid single-character tokens are digits.
        ''|[!0-9]) _appd_throw_error "EXPECTED value GOT ${token:-EOF}" ;;
        *) value=$token
            ;;
    esac
    [ "$value" = '' ] &amp;&amp; return
    _appd_pathMap_add "$jpath" "$value"
    :
}

function _appd_parse () {
    read -r token
    _appd_parse_value
    read -r token
    case "$token" in
        '') ;;
        *) _appd_throw_error "EXPECTED EOF GOT $token" ;;
    esac
}

function _appd_convert_to_script_string () {
    case `echo "$1" | tr A-Z a-z` in
        "true") echo true ;;
        "false") echo false ;;
        "null") echo "" ;;
        \"*\") echo "${1:1:$((${#1}-2))}" ;;
        *) echo "$1" 
    ;;
    esac
}

#
# This method will parse JSON String, get value of given JSON keypath.
# The result will saved in variable $JSONVALUE
# The returned value type will be:
#     String: whenever (1) the value of given JSON keypath is a JSON string (enclosed by double quotation)
#                   or (2) the value of given JSON keypath is a JSON object (enclosed by brace)
#     Number:  whenever the value of given JSON keypath is a number
#     Keyword: whenever the value of given JSON keypath is true/false/null
#     Array: whenever the value of given JSON keypath is a JSON array (enclosed by square)
#
# This method will automatically remove enclosing double quotation when return String or Array of String
#
# Example:
#     getValueFromJson '{"key1":"value1", "key2":{"subkey1":"subValue1"}}' 'key2.subkey1'
#     $JSONVALUE will be "subValue1"
# Example:
#     getValueFromJson '{"key1":["value1","value2"]}' 'key1[1]'
#     $JSONVALUE will be "value2"
# Example:
#     getValueFromJson '[{"key1":"value1"}, {"key1":"value2"}]' '[1].key1'
#     $JSONVALUE will be "value2"
# Example:
#     getValueFromJson '[{"key1":"value1"}, {"key1":"value2"}]' ''
#     $JSONVALUE will be an array with 2 elements.
#     first element is  '{"key1":"value1"}'
#     second element is '{"key1":"value2"}'
# Example:
#     getValueFromJson '{"key1":"value1"}' ''
#     $JSONVALUE will be same as input string.
#
function getValueFromJson () {
    local JSONSTRING=$1
    local JSONPATH=$2
    local tokenTmp
    local stringValue

    _appd_pathMap_init
    tokenTmp=`mktemp`
    echo "$JSONSTRING" | _appd_tokenize &gt; $tokenTmp
    _appd_parse &lt; $tokenTmp
    rm -rf tokenTmp
    stringValue=`_appd_pathMap_get $JSONPATH`
    if [[ $stringValue == \[* ]] ;
    then
        local length
        JSONVALUE=()
    length=$((${#pathArray[@]}-1))
    for i in `eval echo "{0..$length}"`;
        do
        if [[ "${pathArray[$i]}" =~ "$JSONPATH" ]];
        then
                local subPath=${pathArray[$i]:${#JSONPATH}}
                if [[ "$subPath" =~ ^\[[0-9]+\]$ ]];
                then
                    local elementValue
                    elementValue=`_appd_convert_to_script_string "${valueArray[i]}"`
                    JSONVALUE=("${JSONVALUE[@]}" "$elementValue")
                fi
            fi
    done
    else
        JSONVALUE=`_appd_convert_to_script_string "$stringValue"`
    fi
}
#End of Json Parser


# Execute action script
source $1
APPDRETURNSTATUS=$?
# Unset trap
trap - EXIT
# Save env variables in output properties file
printenv &gt; $2
exit $APPDRETURNSTATUS
</exec-script>
